# EmergencyGuard Development Environment

# Server configuration
PORT=3000
NODE_ENV=development
SERVER_URL=http://localhost:3000

# Database configuration
DATABASE_TYPE=sqlite
SQLITE_DATABASE_PATH=./database/emergency_guard.db

# Demo mode (safe for development)
DEMO_MODE=true

# Log level
LOG_LEVEL=debug

# Ping message
PING_MESSAGE=EmergencyGuard Development Server

# Mock notification delay
MOCK_NOTIFICATION_DELAY=500
